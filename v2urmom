-- Scripts:

local function VRDIWMM_fake_script() -- Border.LocalScript 
	local script = Instance.new('LocalScript', Border)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
	
		counter = counter + 0.01
	end
	
end
coroutine.wrap(VRDIWMM_fake_script)()
local function JWOH_fake_script() -- Border_2.LocalScript 
	local script = Instance.new('LocalScript', Border_2)

