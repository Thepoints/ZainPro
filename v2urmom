local function MCMW_fake_script() -- Home.LocalScript 
	local script = Instance.new('LocalScript', Home)

	script.Parent.MouseButton1Click:Connect(function(WaypointsOpen)
		local Target = script.Parent.Parent.DisplayHomeFrame
		wait()
		if Target.Visible == true then
			Target.Visible = false
	
		elseif Target.Visible == false then
			Target.Visible = true
		end	
	end)
	
	
	
end
coroutine.wrap(MCMW_fake_script)()
local function ROKOFDT_fake_script() -- MainFrame.LocalScript 
	local script = Instance.new('LocalScript', MainFrame)

	    frame = script.Parent.MainFrame
	    frame.Draggable = true
	    frame.Active = true
	    frame.Selectable = true
end
coroutine.wrap(ROKOFDT_fake_script)()
local function GIULAF_fake_script() -- DisplayHomeFrame.LocalScript 
	local script = Instance.new('LocalScript', DisplayHomeFrame)

	local image = script.Parent:WaitForChild("ImageLabel")
	
	image.Image = game.Players:GetUserThumbnailAsync(game.Players.LocalPlayer.UserId,Enum.ThumbnailType.HeadShot,Enum.ThumbnailSize.Size420x420)
end
coroutine.wrap(GIULAF_fake_script)()
